{
  "name": "Experibot",
  "version": "1.0.0",
  "msteams": {
    "teamsAppId": "fa3e49b3-5bf1-46ea-bfc0-3e2fd370ed7f"
  },
  "description": "Microsoft Teams conversation bot quickstart",
  "author": "Microsoft",
  "license": "MIT",
  "main": "dist/application/index.js",
  "scripts": {
    "bns": "npm run build && npm start",
    "start": "node dist/application/index.js",
    "watch": "nodemon ./index.js",
    "copy-static": "echo Copy static && ncp src/frontend/static/ dist/application/static/",
    "build": "echo Build && tsc && npm run copy-static && npm run webpack",
    "clean": "rimraf dist/",
    "rebuild": "npm run clean && npm run build",
    "prepublish": "npm run rebuild",
    "test": "mocha --require ts-node/register ./test/*.test.ts",
    "test-js": "mocha ./test/*.test.js",
    "webpack": "echo 'Webpack' && webpack"
  },
  "dependencies": {
    "@fluentui/react": "^8.94.3",
    "@microsoft/teams-js": "^2.3.0",
    "botbuilder": "^4.12.0",
    "botbuilder-dialogs": "^4.13.1",
    "dotenv": "^8.0.0",
    "express": "~4.17.1",
    "html-entities": "^1.3.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.1.4",
    "node-fetch": "^2.6.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/microsoftteams": "^1.9.2",
    "@types/mocha": "^8.2.2",
    "@types/node": "^14.14.37",
    "@types/node-fetch": "^2.5.10",
    "@types/react": "<18.0.0",
    "@types/react-dom": "<18.0.0",
    "@types/should": "^13.0.0",
    "@types/uuid": "^8.3.0",
    "botframework-directlinejs": "^0.15.0",
    "mocha": "^8.3.2",
    "ncp": "^2.0.0",
    "react": "<18.0.0",
    "react-dom": "<18.0.0",
    "rimraf": "^3.0.2",
    "should": "^13.2.3",
    "testdouble": "^3.16.1",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "ws": "^8.3.0",
    "xhr2": "^0.2.1"
  }
